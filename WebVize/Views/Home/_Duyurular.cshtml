@using WebVize.Models.Entity
@model List<Duyurular>
@{
    ViewBag.Title = "_Duyurular";
}
<div id="menu" class="menu">
    <section id="schedule" class="section-with-bg">
        <div class="container" data-aos="fade-up">
            <ul class="nav nav-tabs" role="tablist" data-aos="fade-up" data-aos-delay="100">
                <li class="nav-item">
                    <a class="nav-link active" href="#day-1" role="tab" data-bs-toggle="tab">Duyurular</a>
                </li>
            </ul>
            <div role="tabpanel" class="col-lg-9 tab-pane fade show active" id="day-1">
                @foreach (var dyr in Model)
                {
                    <div class="row schedule-item">
                        <div class="col-md-2"><time>@dyr.DuyuruTarih</time></div>
                        <div class="col-md-10">
                            <h4>@dyr.DuyuruBasligi</h4>
                            <p>@dyr.Duyuru</p>
                        </div>
                    </div>
                }
            </div>
            <!-- End Schdule Day 1 -->
        </div>
    </section>
</div>

